FC                      1. Inport	EQU	$FC
---- File: "lab4-4.sflisp"
FB                      2. Utport	EQU	$FB
05                      3. DelayC1	EQU	$5		;Dessa konstanter styr hur lång fördröjning Delay1s orsakar.
02                      4. DelayC2	EQU	$2		;Ändra dem eventuellt i labbet så att fördröjnigen blir rimligt lång.
                        5. 
FF                      6. 	ORG	$FF
FF 20                   7. 	FCB	$20
                        8. 
20                      9. 	ORG	$20
20 92 20               10. start:	LDSP	#$20
22 34 26               11. 	JSR	main
24 33 20               12. 	JMP	start
                       13. 
26                     14. main:
26 35 FB               15. 	CLR	Utport
28 34 50               16. 	JSR	ReadAscii
2A 94 41               17. 	SUBA	#$41
2C 34 41               18. 	JSR	checkA
2E 0B                  19. 	LSLA
2F 10                  20. 	PSHA
30 90 89               21. 	LDX	#MorseCode
32 F4                  22. 	LDA	A,X
33 E1 88               23. 	STA	Count
35 14                  24. 	PULA
36 07                  25. 	INCA
37 F4                  26. 	LDA	A,X
38                     27. loop2:
38 34 60               28. 	JSR	BlinkLight
3A 0B                  29. 	LSLA
3B 38 88               30. 	DEC	Count
3D 25 F9               31. 	BNE   	loop2
3F 21 E5               32. 	BRA	main
                       33. 
                       34. 
41 10                  35. checkA:	PSHA
42 22 06               36. 	BMI	error
44 94 19               37. 	SUBA	#$19
46 2C 02               38. 	BGT	error
48 14                  39. 	PULA
49 43                  40. 	RTS
4A 35 FB               41. error:	CLR	Utport
4C 38 FB               42. 	DEC	Utport
4E 21 FA               43. 	BRA	error
                       44. 
                       45. 
50                     46. ReadAscii:
50                     47. wait1:
50 F1 FC               48. 	LDA	Inport
52 99 01               49. 	ANDA	#01
54 25 FA               50. 	BNE	wait1
56 F1 FC               51. wait2	LDA	Inport
58 99 01               52. 	ANDA	#01
5A 24 FA               53. 	BEQ	wait2
5C F1 FC               54. 	LDA	Inport
5E 0C                  55. 	LSRA
5F 43                  56. 	RTS
                       57. 
                       58. 
60                     59. 	
60                     60. BlinkLight:
60 10                  61. 	PSHA
61 35 FB               62. 	CLR	Utport
63 38 FB               63. 	DEC	Utport
65 09                  64. 	TSTA
66 2D 06               65. 	BGE	skip
68 34 74               66. 	JSR	Delay1s
6A 34 74               67. 	JSR	Delay1s
6C 34 74               68. 	JSR	Delay1s
6E 34 74               69. skip:	JSR	Delay1s
70 35 FB               70. 	CLR 	Utport
72 14                  71. 	PULA
73 43                  72. 	RTS
74                     73. 	
74                     74. 	
                       75. ; Orsakar en fördröjning på ungefär 1s i simulatorn (vid snabb exekveringshastighet)
                       76. ; Ändrar inga register
74                     77. Delay1s:
74 10                  78. 	PSHA
75 11                  79. 	PSHX
76 13                  80. 	PSHC
77 90 02               81. 	LDX	#DelayC2
79                     82. Delay1s_loop2:
79 F0 05               83. 	LDA	#DelayC1
7B                     84. Delay1s_loop:
7B 08                  85. 	DECA
7C 25 FD               86. 	BNE	Delay1s_loop
7E CC FF               87. 	LEAX	-1,X
80 9C 00               88. 	CMPX	#0
82 25 F5               89. 	BNE	Delay1s_loop2
84 17                  90. 	PULC
85 15                  91. 	PULX
86 14                  92. 	PULA
87 43                  93. 	RTS
                       94. 
                       95. ; Data - Variabler
88                     96. Count:
88 00                  97. 	RMB	1
                       98. 
                       99. ; Data - Konstanter
89                    100. MorseCode:
